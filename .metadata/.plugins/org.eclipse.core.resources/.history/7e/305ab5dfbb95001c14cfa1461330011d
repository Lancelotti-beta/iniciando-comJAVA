
public class TestaLoop {

	public static void main(String[] args) {
		
		int number = 0;
		int cont = 0;
		
		System.out.println("Teste do While");
		while (number < 5) {
			System.out.println(number);
			++number; 
		}
		
		System.out.println("______");
		
		System.out.println("Teste do Do/While");
		do {
			System.out.println(cont);
			cont++;
		} while (cont < 5);
	}
}
